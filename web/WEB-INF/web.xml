<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>ConnServlet</servlet-name>
        <servlet-class>uts.isd.controller.ConnServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PaymentRegisterServlet</servlet-name>
        <servlet-class>uts.isd.controller.PaymentRegisterServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PaymentEditServlet</servlet-name>
        <servlet-class>uts.isd.controller.PaymentEditServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PaymentViewServlet</servlet-name>
        <servlet-class>uts.isd.controller.PaymentViewServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PaymentDeleteServlet</servlet-name>
        <servlet-class>uts.isd.controller.PaymentDeleteServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PaymentUpdateServlet</servlet-name>
        <servlet-class>uts.isd.controller.PaymentUpdateServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PaymentSearchServlet</servlet-name>
        <servlet-class>uts.isd.controller.PaymentSearchServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ConnServlet</servlet-name>
        <url-pattern>/ConnServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PaymentRegisterServlet</servlet-name>
        <url-pattern>/PaymentRegisterServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PaymentEditServlet</servlet-name>
        <url-pattern>/PaymentEditServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PaymentViewServlet</servlet-name>
        <url-pattern>/PaymentViewServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PaymentDeleteServlet</servlet-name>
        <url-pattern>/PaymentDeleteServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PaymentUpdateServlet</servlet-name>
        <url-pattern>/PaymentUpdateServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PaymentSearchServlet</servlet-name>
        <url-pattern>/PaymentSearchServlet</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>/index.jsp</welcome-file>
    </welcome-file-list>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
